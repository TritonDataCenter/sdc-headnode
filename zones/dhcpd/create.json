{
  "alias": "dhcpd0",
  "brand": "joyent-minimal",
  "filesystems": [
    {
      "source": "/usbkey/os",
      "target": "/tftpboot/os",
      "type": "lofs",
      "options": [
        "ro",
        "nodevices"
      ]
    }
  ],
  "tags": {
      "smartdc_role": "dhcpd",
      "smartdc_type": "core"
  },
  "zfs_storage_pool_name": "zones",
  "registration": {
    "domain": "dhcpd",
    "type": "rr_host",
    "service": {
      "type": "service",
      "service": {
        "srvce": "_dhcpd",
        "proto": "_tcp",
        "ttl": 60,
        "port": 80
      }
    },
    "ttl": 60
  }

}
