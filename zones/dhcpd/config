create -b
set zonepath=/zones/dhcpd
set brand=joyent
set autoboot=true
set ip-type=exclusive
add fs
set dir=/tftpboot/os
set special=/usbkey/os
set type=lofs
add options ro
add options nodevices
end
add net
set physical=dhcpd0
set vlan-id=0
set global-nic=admin
add property (name=dhcp_server,value=1)
end
