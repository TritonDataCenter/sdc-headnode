default=0
timeout=5
min_mem64 1024
color cyan/blue white/blue

title Live 64-bit
   kernel /platform/i86pc/kernel/amd64/unix -B console=text,headnode=true
   module /platform/i86pc/amd64/boot_archive

title Live 64-bit KMDB
   kernel /platform/i86pc/kernel/amd64/unix -kd -B console=text,headnode=true
   module /platform/i86pc/amd64/boot_archive

title Live 64-bit Serial (ttyb)
   kernel /platform/i86pc/kernel/amd64/unix -B console=ttyb,ttyb-mode="115200,8,n,1,-",headnode=true
   module /platform/i86pc/amd64/boot_archive

title Live 64-bit Serial KMDB (ttyb)
   kernel /platform/i86pc/kernel/amd64/unix -kd -B console=ttyb,ttyb-mode="115200,8,n,1,-",headnode=true
   module /platform/i86pc/amd64/boot_archive

title Live 64-bit Serial (ttya)
   kernel /platform/i86pc/kernel/amd64/unix -B console=ttya,ttya-mode="115200,8,n,1,-",headnode=true
   module /platform/i86pc/amd64/boot_archive

title Live 64-bit Serial KDMB (ttya)
   kernel /platform/i86pc/kernel/amd64/unix -kd -B console=ttya,ttya-mode="115200,8,n,1,-",headnode=true
   module /platform/i86pc/amd64/boot_archive

title Live 64-bit Rescue (no importing zpool)
   kernel /platform/i86pc/kernel/amd64/unix -B console=text,headnode=true,noimport=true
   module /platform/i86pc/amd64/boot_archive
